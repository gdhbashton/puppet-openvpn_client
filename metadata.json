{
  "name": "thejandroman-openvpn_client",
  "version": "0.1.1",
  "author": "thejandroman",
  "summary": "Install and configure openvpn client.",
  "license": "Apache-2.0",
  "source": "https://github.com/thejandroman/puppet-openvpn_client.git",
  "project_page": "https://github.com/thejandroman/puppet-openvpn_client",
